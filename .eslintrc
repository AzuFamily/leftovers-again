{
  "extends": "airbnb-base",
  "rules": {
    "comma-dangle": 0,
    "no-console": 0,
    "no-use-before-define": 0,
    "no-nested-ternary": 0,
    "prefer-template": 0,
    "no-param-reassign": 0,
    "no-mixed-operators": 0,
    "import/no-dynamic-require": 0,
    "global-require": 0,
    "no-useless-constructor": 1,
    "no-plusplus": 0,
    "class-methods-use-this": 0
  }
}
