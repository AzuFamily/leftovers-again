{
  "name": "leftovers-again",
  "version": "0.3.0",
  "description": "Pokemon Showdown - Node Client",
  "main": "src/main.js",
  "scripts": {
    "docs": "./node_modules/.bin/esdoc -c esdoc.json",
    "test": "NODE_PATH='src:bots:spec' jasmine",
    "test:coverage": "NODE_PATH='src:bots:spec' istanbul cover  -x '**/spec/**' jasmine",
    "develop": "NODE_PATH='src:bots' nodemon --watch src --watch bots --exec babel-node src/main.js",
    "start": "NODE_PATH='src:bots' babel-node src/main.js",
    "spawn": "NODE_PATH='src:bots' babel-node scripts/spawn.js",
    "battle": "NODE_PATH='src:bots' babel-node scripts/battle.js",
    "generate": "NODE_PATH='src:bots' babel-node scripts/generate.js",
    "server": "cd lib/Pokemon-Showdown && node app.js",
    "client": "./scripts/open-client.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dramamine/leftovers-again.git"
  },
  "keywords": [
    "pokemon"
  ],
  "author": "marten",
  "license": "BSD-2-Clause",
  "dependencies": {
    "babel": "~5.8.23",
    "chalk": "^1.1.1",
    "colors": "~1.1.2",
    "glob": "~5.0.14",
    "minimist": "~1.2.0",
    "request": "~2.61.0",
    "websockets": "~0.2.0",
    "ws": "~0.8.0"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.1",
    "cheerio": "~0.19.0",
    "esdoc": "^0.4.3",
    "eslint": "^1.3.1",
    "eslint-config-airbnb": "0.0.8",
    "handlebars": "^4.0.5",
    "inquirer": "~0.10.1",
    "istanbul": "^0.4.2",
    "jQuery": "~1.7.4",
    "jasmine": "^2.3.2",
    "jasmine-es6": "0.0.17",
    "jsdom": "~3.1.2",
    "lodash": "^3.10.1",
    "nodemon": "~1.4.1"
  }
}
