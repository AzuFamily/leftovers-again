{
  "name": "leftovers-again",
  "version": "0.6.2",
  "description": "Pokemon Showdown - Bot Client",
  "main": "lib/main.js",
  "files": [
    "lib",
    "templates",
    "CONTRIBUTING.md",
    "README.md",
    "FAQ.md"
  ],
  "scripts": {
    "build": "babel src -d lib && babel scripts -d lib/scripts && cp src/data lib -R && (cd src && find ./ -type f \\( -name '*.json' -o -name '*.txt' \\) | tar -cf - -T - | tar -xf - -C ../lib -v)",
    "clean": "rm -rf lib",
    "docs": "./node_modules/.bin/esdoc -c esdoc.json",
    "docs:publish": "curl --data \"gitUrl=git@github.com:dramamine/leftovers-again.git\" https://doc.esdoc.org/api/create",
    "docs:update": "npm run docs:publish",
    "test": "jasmine",
    "test:coverage": "NODE_PATH='src:bots:spec' istanbul cover  -x '**/spec/**' jasmine",
    "develop": "NODE_PATH='src' nodemon --watch src --exec babel-node src/start.js",
    "start": "npm run clean && npm run build && node lib/start.js",
    "start:quick": "node lib/start.js",
    "start:old": "NODE_PATH='src:bots' babel-node src/start.js",
    "spawn:old": "NODE_PATH='src:bots' babel-node scripts/spawn.js",
    "battle:old": "NODE_PATH='src:bots' babel-node scripts/battle.js",
    "generate:old": "NODE_PATH='src:bots' babel-node scripts/generate.js",
    "server": "cd deps/Pokemon-Showdown && node app.js",
    "client": "./scripts/open-client.sh",
    "generate": "./lib/scripts/generate.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dramamine/leftovers-again.git"
  },
  "bugs": "https://github.com/dramamine/leftovers-again/issues",
  "keywords": [
    "pokemon"
  ],
  "author": "marten",
  "license": "BSD-2-Clause",
  "dependencies": {
    "babel": "^6.5.2",
    "babel-plugin-module-alias": "^1.4.0",
    "babel-resolve-relative-module": "^1.0.1",
    "glob": "^7.0.3",
    "handlebars": "^4.0.5",
    "inquirer": "^1.0.2",
    "minimist": "~1.2.0",
    "ws": "^1.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-eslint": "^5.0.0",
    "babel-preset-es2015-node6": "^0.2.0",
    "cheerio": "^0.20.0",
    "esdoc": "^0.4.6",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-react": "^4.3.0",
    "jasmine-es6": "^0.1.10",
    "lodash": "^4.6.1"
  },
  "engine": "node >= 6.1.0"
}
