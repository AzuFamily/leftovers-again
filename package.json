{
  "name": "leftovers-again",
  "version": "0.2.0",
  "description": "Pokemon Showdown - Node Client",
  "main": "src/main.js",
  "scripts": {
    "test": "NODE_PATH='src:bots:spec' jasmine",
    "develop": "NODE_PATH='src:bots' nodemon --watch src --exec babel-node src/main.js",
    "spawn": "NODE_PATH='src:bots' babel-node scripts/spawn.js",
