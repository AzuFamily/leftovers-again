  "license": "BSD-2-Clause",
  "dependencies": {
    "babel": "~5.8.23",
    "request": "~2.61.0",
    "websockets": "~0.2.0",
    "ws": "~0.8.0",
    "colors": "~1.1.2",
    "glob": "~5.0.14",
    "minimist": "~1.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.1",
    "cheerio": "~0.19.0",
    "eslint": "^1.3.1",
    "eslint-config-airbnb": "0.0.8",
    "jQuery": "~1.7.4",
    "jasmine": "^2.3.2",
    "jasmine-es6": "0.0.17",
    "jsdom": "~3.1.2",
    "lodash": "^3.10.1",
    "nodemon": "~1.4.1",
    "inquirer": "~0.10.1"
